{
    "manifest_version": 3,
    "name": "Manaba Plus",
    "version": "$version$",
    "description": "Add awesome modifications to manaba",
    "permissions": [
      "storage",
      "downloads"
    ],
    "icons": {
      "16": "img/icon-mp-16.png",
      "32": "img/icon-mp-32.png",
      "48": "img/icon-mp-48.png",
      "128": "img/icon-mp-128.png"
    },
    "action": {
      "default_title": "Manaba Plusを開く",
      "default_popup": "popup/index.html"
    },
    "options_ui": {
      "page": "options/index.html"
    },
    "content_scripts": [
      {
        "matches": [
          "$bad-status$"
        ],
        "js": [
          "bad-status/index.js"
        ],
        "css": [
          "bad-status/style.css"
        ]
      },
      {
        "matches": [
          "$manaba-url$*"
        ],
        "js": [
          "common/index.js"
        ],
        "css": [
          "common/style.css"
        ]
      },
      {
        "matches": [
          "$manaba-url$course*_grade"
        ],
        "js": [
          "grade/index.js"
        ],
        "css": [
          "grade/style.css"
        ]
      },
      {
        "matches": [
          "$manaba-url$home*"
        ],
        "exclude_matches": [
          "$manaba-url$home_coursetable",
          "$manaba-url$home_selfregistration_*"
        ],
        "js": [
          "home/index.js"
        ],
        "css": [
          "home/style.css"
        ]
      },
      {
        "matches": [
          "$manaba-url$page_*"
        ],
        "js": [
          "page/index.js"
        ],
        "css": [
          "page/style.css"
        ]
      },
      {
        "matches": [
          "$manaba-url$course*_report_*"
        ],
        "js": [
          "report/index.js"
        ],
        "css": [
          "report/style.css"
        ]
      },
      {
        "matches": [
          "$manaba-url$syllabus_*"
        ],
        "js": [
          "syllabus/index.js"
        ],
        "css": [
          "syllabus/style.css"
        ]
      },
      {
        "matches": [
          "https://hotaritobu.github.io/manaba-plus/reminders/google-calendar/"
        ],
        "js": [
          "reminders/google-calendar/index.js"
        ]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "contents/index.html",
          "options/index.html"
        ],
        "matches": [
          "$manaba-url$"
        ]
      }
    ]
  }