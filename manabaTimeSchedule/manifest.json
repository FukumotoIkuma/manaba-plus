{
    "name":"manaba時間割",
    "version":"1.1.3",
    "manifest_version":3,
    "description": "manbaのコースを時間割に並び替え",
    "permissions": [
      "storage"
    ],
    "content_scripts": [{
        "matches": [
          "https://manaba.lms.tokushima-u.ac.jp/ct/*"
      ],
        "js": [
          "loader.js"
        ],
        "css":[
          "css/courseCard.css"
        ]
    }],
    "action":{
        "default_icon":"icon.png"
    },
    "web_accessible_resources":[
      {
        "resources":[
          "/js/content.js",
          "/js/createCourseCard.js",
          "/js/courseCardDrag.js",
          "/js/table.js"
        ],
        "matches":["<all_urls>"]
      }
    ],
    "options_ui": {
      "page": "html/options.html",
      "open_in_tab": false
  }

}